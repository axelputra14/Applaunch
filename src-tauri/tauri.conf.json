{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": ["../src/assets/*"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Applaunch",
  "mainBinaryName": "Applaunch",
  "version": "0.0.0",
  "identifier": "com.applaunch.dev",
  "plugins": {},
  "app": {
    "security": {
      "assetProtocol": {
        "scope": ["**"],
        "enable": true
      },
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "applaunch",
        "width": 1600,
        "height": 900,
        "decorations": false,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay"
      }
    ]
  }
}
